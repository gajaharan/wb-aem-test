<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:description="Template with focus on test content."
          jcr:primaryType="cq:Template"
          jcr:title="WB Test Generic Content Page"
          allowedPaths="[/content/wb-aem-test-app]"
          ranking="{Long}100">
    <jcr:content
            jcr:primaryType="cq:PageContent"
            sling:resourceType="wb-aem-test-app/components/structure/page/test-page">
        <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wb-aem-test-app/components/structure/header" />

        <par-one
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys" />

        <par-two
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys" />

        <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wb-aem-test-app/components/structure/footer" />
    </jcr:content>
</jcr:root>